{
  "site": {
    "title": "Conservare Gráfica - Impressões que preservam sua marca.",
    "base_url": "https://astroplate.netlify.app",
    "base_path": "/",
    "trailing_slash": false,
    "favicon": "/images/favicon.png",
    "logo": "/images/logo-darkmode.svg",
    "logo_darkmode": "/images/logo.svg",
    "logo_width": "180",
    "logo_height": "45",
    "logo_text": "Conservare Gráfica"
  },

  "settings": {
    "search": true,
    "sticky_header": true,
    "theme_switcher": true,
    "default_theme": "system",
    "pagination": 4,
    "summary_length": 100,
    "blog_folder": "produtos"
  },

  "params": {
    "contact_form_action": "#",
    "copyright": "2023 - Conservare Gráfica ® | CNPJ: 22.367.488/0001-22 | Todos os direitos reservados."
  },

  "navigation_button": {
    "enable": true,
    "label": "FALAR COM ATENDENTE",
    "link": "https://github.com/zeon-studio/astroplate"
  },

  "disqus": {
    "enable": true,
    "shortname": "themefisher-template",
    "settings": {
      "language": "pt_BR"
    }
  },

  "metadata": {
    "meta_author": "Conservare Gráfica",
    "meta_image": "/images/og-image.png",
    "meta_description": "astro and tailwind boilerplate"
  }
}
